   "source": [
    "def permute(l, s):\n",
    "    if l == 1:\n",
    "        return s\n",
    "    else:\n",
    "        return [x + y for x in permute(1, s)\n",
    "                for y in permute(l - 1, s)]\n",
    "\n",
    "\n",
    "print(permute(1, ['a', 'b', 'c', 'd']))\n",
    "print(permute(3, ['a', 'b', 'c', 'd']))"
   ]
